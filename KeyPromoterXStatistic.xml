<application>
  <component name="KeyPromoterXStatistic">
    <option name="statistics">
      <Statistic Action="Copy Path">
        <Item>
          <IdeaActionID>CopyPaths</IdeaActionID>
          <ShortCut>'⇧⌘C'</ShortCut>
          <Description>Copy Path</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Declaration">
        <Item>
          <IdeaActionID>GotoDeclaration</IdeaActionID>
          <ShortCut>'⌘B'</ShortCut>
          <Description>Declaration</Description>
          <Count>7</Count>
        </Item>
      </Statistic>
      <Statistic Action="Delete...">
        <Item>
          <IdeaActionID>$Delete</IdeaActionID>
          <ShortCut>'⌦' or '⌫' or '⌘⌫'</ShortCut>
          <Description>Delete...</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Push...">
        <Item>
          <IdeaActionID>Vcs.Push</IdeaActionID>
          <ShortCut>'⇧⌘K'</ShortCut>
          <Description>Push...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Rename...">
        <Item>
          <IdeaActionID>RenameElement</IdeaActionID>
          <ShortCut>'⇧F6'</ShortCut>
          <Description>Rename...</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Run 'Tester (1)'">
        <Item>
          <IdeaActionID>Run</IdeaActionID>
          <ShortCut>'⌃R'</ShortCut>
          <Description>Run 'Tester (1)'</Description>
          <Count>3</Count>
        </Item>
      </Statistic>
      <Statistic Action="Run 'Tester.main()'">
        <Item>
          <IdeaActionID>RunClass</IdeaActionID>
          <ShortCut>'⌃⇧R'</ShortCut>
          <Description>Run 'Tester.main()'</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
    </option>
  </component>
</application>